YII_DATETIME \d{4}(?:\-\d{1,2}){2}\s\d{1,2}(?::\d{1,2}){2}
YII_IP \[%{IP}\]
YII_USER_ID (?:[^\]]*)
YII_SESSION_ID (?:[^\]]*)
YII_SEVERITY_LEVEL (?:[^\]]*)
YII_CATEGORY (?:[^\]]*)
YII_LOG_CONTENT (?:.*)
